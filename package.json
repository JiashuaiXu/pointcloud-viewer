{
  "name": "pointcloud-viewer",
  "version": "1.0.0",
  "description": "A point cloud viewer using Potree, Vue/React and TypeScript",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:vue": "vite --config vite.config.vue.ts",
    "dev:react": "vite --config vite.config.react.ts",
    "build": "vite build",
    "build:vue": "vue-tsc && vite build --config vite.config.vue.ts",
    "build:react": "tsc && vite build --config vite.config.react.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.4.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.160.0",
    "potree-core": "^1.8.0"
  },
  "devDependencies": {
    "@types/three": "^0.160.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-vue": "^5.0.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vue-tsc": "^1.8.27"
  }
}
